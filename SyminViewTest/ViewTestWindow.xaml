<Window x:Class="SyminViewTest.ViewTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SyminViewTest"
        xmlns:sv ="clr-namespace:SyminUI;assembly=SyminUI"
        xmlns:v ="clr-namespace:SyminViewTest.Views"
        mc:Ignorable="d"
        Title="ViewTestWindow" Height="450" Width="800">
    <Grid Background="{StaticResource Background.Main}">
        <Grid.Resources>
            <v:TestView x:Key="TV"/>
        </Grid.Resources>
        <sv:ViewBody ViewDefinition="{StaticResource ResourceKey=TV}"/>
    </Grid>
</Window>
